{
  "name": "kreateandco",
  "private": true,
  "scripts": {
    "dev:api": "node apps/api/index.js",
    "dev:web": "vite --config apps/web/vite.config.cjs",
    "dev": "concurrently -k \"npm:dev:api\" \"npm:dev:web\"",
    "build": "vite build --config apps/web/vite.config.cjs",
    "start": "concurrently -k \"npm:dev:api\" \"vite preview --config apps/web/vite.config.cjs --host 0.0.0.0 --port 54321\"",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier . --check",
    "format:write": "prettier . --write"
  },
  "dependencies": {
    "@dbos-inc/dbos-sdk": "^4.7.9",
    "@google-cloud/storage": "^7.18.0",
    "@tanstack/react-query": "^5.90.20",
    "@uppy/aws-s3": "^5.1.0",
    "@uppy/core": "^5.2.0",
    "@uppy/dashboard": "^5.1.1",
    "@uppy/react": "^5.2.0",
    "dotenv": "^16.4.7",
    "google-auth-library": "^10.5.0",
    "pg": "^8.13.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.13.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^9.1.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^4.6.2",
    "prettier": "^3.5.1",
    "vite": "^5.4.14"
  }
}
